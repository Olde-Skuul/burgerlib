#

A = $(BASE_TEMP_DIR)watw32dbg
bin\unittestswatw32dbg.exe: $+$(OBJS)$- unittestswatw32.wmk
	@SET WOW={$+$(OBJS)$-}
	@$(LINK) $(LFlagsDebugw32) NAME $^@ FILE @wow

A = $(BASE_TEMP_DIR)watw32int
bin\unittestswatw32int.exe: $+$(OBJS)$- unittestswatw32.wmk
	@SET WOW={$+$(OBJS)$-}
	@$(LINK) $(LFlagsInternalw32) NAME $^@ FILE @wow

A = $(BASE_TEMP_DIR)watw32rel
bin\unittestswatw32rel.exe: $+$(OBJS)$- unittestswatw32.wmk
	@SET WOW={$+$(OBJS)$-}
	@$(LINK) $(LFlagsReleasew32) NAME $^@ FILE @wow
